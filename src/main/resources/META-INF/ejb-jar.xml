<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar id="ejb-jar_ID" version="2.1"
	xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
	<display-name>EJBDemo</display-name>
	<enterprise-beans>
		<session>
			<ejb-name>CalculatorEJB</ejb-name>
			<home>tdcc.lab.simple.CalculatorHome</home>
			<remote>tdcc.lab.simple.CalculatorRemote</remote>
			<ejb-class>tdcc.lab.simple.CalculatorEJB</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session>
			<ejb-name>CategorySessionBean</ejb-name>
			<home>tdcc.lab.category.CategoryServiceHome</home>
			<remote>tdcc.lab.category.CategoryServiceRemote</remote>
			<ejb-class>tdcc.lab.category.CategorySessionBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session>
			<ejb-name>CalculatorEJB3</ejb-name>
			<remote>tdcc.lab.simple.ejb3.Calculator</remote>
			<ejb-class>tdcc.lab.simple.ejb3.CalculatorEJB</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
		<session>
			<ejb-name>CategorySessionBeanEJB3</ejb-name>
			<remote>tdcc.lab.category.ejb3.CategoryService</remote>
			<ejb-class>tdcc.lab.category.ejb3.CategorySessionBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>
</ejb-jar>
